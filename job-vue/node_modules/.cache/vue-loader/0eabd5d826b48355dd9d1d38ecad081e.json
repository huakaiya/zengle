-col cols=\"12\" md=\"6\">\n                  <v-card-text class=\"mt-12\">\n                    <h1 class=\"text-center\">\n\n                      IT类人才招聘画像数据可视化分析系统\n                    </h1>\n                    <h3\n                        class=\"text-center  grey--text mt-4\"\n                    >输入昵称和密码进行登录 <br></h3>\n                    <v-row align=\"center\" justify=\"center\">\n                      <v-col cols=\"12\" sm=\"8\">\n                        <v-text-field\n                            label=\"昵称\"\n                            outlined\n                            dense\n                            color=\"blue\"\n                            autocomplete=\"false\"\n                            class=\"mt-16\"\n                            v-model=\"username\"\n                        />\n                        <v-text-field\n                            label=\"密码\"\n                            outlined\n                            dense\n                            color=\"blue\"\n                            autocomplete=\"false\"\n                            type=\"password\"\n                            v-model=\"password\"\n\n                        />\n<!--                        <v-row>-->\n<!--                          <v-col cols=\"12\" sm=\"7\">-->\n<!--                            <v-checkbox-->\n\n<!--                                label=\"记住我\"-->\n<!--                                class=\"mt-n1\"-->\n<!--                                color=\"blue\"-->\n<!--                            ></v-checkbox>-->\n<!--                          </v-col>-->\n\n<!--                        </v-row>-->\n                        <v-btn color=\"blue\" dark block tile @click=\"login()\">登录</v-btn>\n\n                      </v-col>\n                    </v-row>\n                  </v-card-text>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\" class=\"green rounded-bl-xl\">\n                  <div style=\"  text-align: center; padding: 180px 0;\">\n                    <v-card-text class=\"white--text\">\n                      <h2 class=\"text-center \">没有账号密码?</h2>\n                      <h4\n                          class=\"text-center\"\n                      >点击下方注册按钮进行注册</h4>\n                    </v-card-text>\n                    <div class=\"text-center\">\n                      <v-btn tile outlined dark @click=\"step++\">注册</v-btn>\n                    </div>\n                  </div>\n                </v-col>\n              </v-row>\n            </v-window-item>\n            <v-window-item :value=\"2\">\n              <v-row>\n                <v-col cols=\"12\" md=\"6\" class=\"blue rounded-br-xl\">\n                  <div style=\"  text-align: center; padding: 180px 0;\">\n                    <v-card-text class=\"white--text\">\n                      <h3 class=\"text-center \">已注册?</h3>\n                      <h6\n                          class=\"text-center\"\n                      >回到登录页面<br> 点击下方</h6>\n                    </v-card-text>\n                    <div class=\"text-center\">\n                      <v-btn tile outlined dark @click=\"step--;\">登录</v-btn>\n                    </div>\n                  </div>\n                </v-col>\n\n                <v-col cols=\"12\" md=\"6\">\n                  <v-card-text class=\"mt-12\">\n                    <h1\n                        class=\"text-center\"\n                    >注册一个账号</h1>\n\n                    <v-row align=\"center\" justify=\"center\">\n                      <v-col cols=\"12\" sm=\"8\">\n<!--                        <v-text-field-->\n<!--                                label=\"昵称\"-->\n<!--                                outlined-->\n<!--                                dense-->\n<!--                                color=\"blue\"-->\n<!--                                autocomplete=\"false\"-->\n<!--                                class=\"mt-4\"-->\n<!--                                v-model=\"realname\"-->\n<!--                        />-->\n                        <v-text-field\n                            label=\"昵称\"\n                            outlined\n                            dense\n                            color=\"blue\"\n                            autocomplete=\"false\"\n                            v-model=\"username\"\n                        />\n                        <v-text-field\n                            label=\"密码\"\n                            outlined\n                            dense\n                            color=\"blue\"\n                            autocomplete=\"false\"\n                            type=\"password\"\n                            v-model=\"password\"\n                        />\n<!--                        <v-row>-->\n<!--                          <v-col cols=\"12\" sm=\"7\">-->\n<!--                            <v-checkbox-->\n\n<!--                                label=\"同意\"-->\n<!--                                class=\"mt-n1\"-->\n<!--                                color=\"blue\"-->\n<!--                            ></v-checkbox>-->\n<!--                          </v-col>-->\n\n<!--                        </v-row>-->\n                        <v-btn color=\"blue\" dark block tile @click=\"register()\">注册</v-btn>\n\n\n                      </v-col>\n                    </v-row>\n                  </v-card-text>\n                </v-col>\n              </v-row>\n            </v-window-item>\n          </v-window>\n        </v-card>\n      </v-col>\n\n      <div class=\"videoContain\">\n        <video loop v-if=\"showVideo\" muted ref=\"my_video\" preload=\"metadata\" class=\"videoStyle\"\n               v-on:canplay=\"canplay\">\n          <source\n              src=\"../assets/video/1.mp4\"\n              type=\"video/webm\"/>\n        </video>\n      </div>\n    </v-row>\n  </v-container>\n  </v-app>\n</template>\n\n<script>\n\nimport {login, register} from \"../api/user\";\nimport mixin from '../mixins/mixins'\n\nexport default {\n  mixins: [mixin],\n  data: () => ({\n    step: 1,\n    vedioCanPlay: false,\n    showVideo: true,\n    username: '',\n    password: '',\n    realname: 'test',\n  }),\n  props: {\n    source: String\n  },\n  mounted: function () {\n    let $ = this;\n    let myVid = $.$refs.my_video;\n    setTimeout(function () {\n      myVid.play();\n    }, 150);\n    myVid.ontimeupdate = function () {\n      if (this.currentTime > 15) {\n        $.dark = true;\n        $.alertType = 'error';\n        $.cardBackground = 'background-color: rgba(0,121,255,0.2);';\n      } else {\n        $.dark = false;\n        $.alertType = 'warning';\n        $.cardBackground = 'background-color: rgba(0,121,255,0.04);';\n      }\n    };\n\n  },\n  methods: {\n    canplay() {\n      this.vedioCanPlay = true\n    },\n    login() {\n      if(this.username == \"\" || this.password == \"\"){\n        this.$snackbar({content: '表单数据未填写完整', top:true, center:true, color:'red',multiLine: true})\n        return false\n      }\n\n      const loginForm = {\"username\": this.username, \"password\": this.password}\n      login(loginForm).then(res=>{\n        const {data} = res\n        if(data.code != 0)\n          this.$snackbar({content: data.msg, top:true, center:true, color:'red',multiLine: true})\n        else {\n          // localStorage.setItem('uid', data.data)\n          this.$store.dispatch('setUserInfo', data.data)\n          this.$snackbar({content: '登录成功', top:true, center:true, color:'green',multiLine: true})\n          this.$router.push('/library')\n        }\n      })\n    },\n    register() {\n  if (\n    this.username == \"\" ||\n    this.password == \"\" ||\n    this.realname == \"\"\n  ) {\n    this.$snackbar({\n      content: '表单数据未填写完整',\n      top: true,\n      center: true,\n      color: 'red',\n      multiLine: true\n    })\n    return false\n  }\n\n  // 添加密码长度和组成要求的判断\n  if (this.password.length < 8 || !(/[A-Za-z].*[0-9]|[0-9].*[A-Za-z]/.test(this.password))) {\n    this.$snackbar({\n      content: '密码错误，组合必须不少于8位，且包含字母和数字',\n      top: true,\n      center: true,\n      color: 'red',\n      multiLine: true\n    })\n    return false\n  }\n\n  const registForm = {\"username\": this.username, \"password\": this.password, \"realname\": this.realname}\n  register(registForm).then(res=>{\n    const {data} = res\n    if(data.code != 0)\n      this.$snackbar({\n        content: data.msg,\n        top: true,\n        center: true,\n        color: 'red',\n        multiLine: true\n      })\n    else {\n      this.$snackbar({\n        content: '注册成功，请登录',\n        top: true,\n        center: true,\n        color: 'green',\n        multiLine: true\n      })\n    }\n  })\n}\n\n  }\n\n\n}\n</script>\n<style scoped>\n.v-application .rounded-bl-xl {\n  border-bottom-left-radius: 300px !important;\n}\n\n.v-application .rounded-br-xl {\n  border-bottom-right-radius: 300px !important;\n}\n</style>\n<style lang=\"stylus\">\n.loginContainer\n  z-index :1;\n\n.videoContain\n  display: inline;\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  top: 0;\n  z-index: 0;\n\n  .videoStyle\n    object-fit: fill;\n    width: 100%;\n    height: 100%;\n</style>\n"]}]}
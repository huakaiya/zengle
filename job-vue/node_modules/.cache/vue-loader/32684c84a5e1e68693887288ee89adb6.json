t-size: 2px;\"></span>\n    </v-card-title>\n\n    <v-card-text>\n      <v-row\n          align=\"center\"\n          class=\"mx-0\"\n      >\n<!--        <v-rating-->\n<!--            :value=\"item.rate/2.0\"-->\n<!--            color=\"amber\"-->\n<!--            dense-->\n<!--            half-increments-->\n<!--            readonly-->\n<!--            size=\"14\"-->\n<!--        ></v-rating>-->\n        <div class=\"grey--text ms-4\">\n          企业： {{item.company_name}} · {{item.city}}\n        </div>\n        <div class=\"grey--text ms-4\">\n          薪酬： {{item.salary0}} - {{item.salary1}}元\n        </div>\n        <div class=\"grey--text ms-4\">\n          企业性质：{{item.coattr}}·{{item.nation}}\n        </div>\n        <div class=\"grey--text ms-4\">\n          学历要求：{{item.degree}}\n        </div>\n        <div class=\"grey--text ms-4\">\n          公司规模：{{item.cosize0}} - {{item.cosize1}}\n        </div>\n      </v-row>\n\n<!--      <div class=\"my-2 text-subtitle-1\">-->\n<!--        企业性质：{{item.coattr}}·{{item.nation}}-->\n<!--      </div>-->\n\n<!--      <div class=\"my-2 text-subtitle-1\">-->\n<!--        学历要求：{{item.degree}}-->\n<!--      </div>-->\n\n      <div>{{item.intro}}</div>\n    </v-card-text>\n\n    <v-divider class=\"mx-4\"></v-divider>\n\n<!--    <v-card-title>公司福利</v-card-title>-->\n\n<!--    <v-card-text>-->\n<!--      <v-chip-group-->\n<!--          v-model=\"selection\"-->\n<!--          active-class=\"deep-purple accent-4 white&#45;&#45;text\"-->\n<!--        column-->\n<!--      >-->\n<!--        <v-chip v-for=\"(i,idx) in item.welfare.split(',')\" :key=\"idx\">{{i}}</v-chip>-->\n<!--      </v-chip-group>-->\n<!--    </v-card-text>-->\n\n    <v-card-actions>\n      <v-btn\n          color=\"deep-purple lighten-2\"\n          text\n          @click=\"reserve(item.company_url)\">\n        公司详情\n      </v-btn>\n      <v-btn\n          color=\"deep-purple accent-2\"\n          text\n          @click=\"reserve(item.url)\">\n        职位详情\n      </v-btn>\n        <v-btn\n    color=\"deep-purple accent-2\"\n    text\n    @click=\"toggleCollect(item)\">\n    {{ item.flag ? '取消收藏' : '收藏' }}\n</v-btn>\n\n    </v-card-actions>\n  </v-card>\n</template>\n\n<script>\n    import {collect} from \"../api/job\";\n    import {cancelCollect} from \"../api/job\";\nexport default {\n  name: \"job-card\",\n  props: {\n    item: Object,\n    cardTitle: String,\n      uid:Number\n  },\n data() {\n  return {\n    loading: false,\n    selection: -1,\n  };\n},\n\n\n  methods: {\n      // 点击按钮触发收藏或取消收藏操作\n    toggleCollect(item) {\n        if (item.flag) {\n            // 如果已收藏，执行取消收藏操作\n            this.cancelCollect(item);\n        } else {\n            // 如果未收藏，执行收藏操作\n            this.collect(item);\n        }\n    },\n    // 收藏职位\n    collect(item) {\n        // 执行收藏操作，你可以在这里向后端发送请求，将职位添加到收藏列表中\n        // 在这里执行你的收藏逻辑，可以调用后端 API 或者更新本地状态\n        // 更新 item.flag 为 true，表示已收藏\n        console.log(this.uid)\n        collect(item.id,1).then(res=>{\n        // this.$emit('search-movie', res.data.data)\n        console.log(res.data.data)\n      })\n        item.flag = true;\n        // 这里可以执行其他操作，比如弹出提示或者更新 UI\n    },\n    // 取消收藏职位\n    cancelCollect(item) {\n        // 执行取消收藏操作，你可以在这里向后端发送请求，将职位从收藏列表中移除\n        // 在这里执行你的取消收藏逻辑，可以调用后端 API 或者更新本地状态\n        // 更新 item.flag 为 false，表示未收藏\n        console.log(this.uid)\n        cancelCollect(item.id,1).then(res=>{\n        // this.$emit('search-movie', res.data.data)\n        console.log(res.data.data)\n      })\n        item.flag = false;\n        // 这里可以执行其他操作，比如弹出提示或者更新 UI\n    },\n    doubanImg(src) {\n      let trueSrc = ''\n      if(src!=null && src.startsWith(\"http://localhost:8080\"))\n        trueSrc = src\n      else if(src!=null && src!='')\n        trueSrc = 'https://images.weserv.nl/?url=' + src\n      else\n        trueSrc = require(\"@/assets/nologo.png\")\n      return trueSrc\n    },\n    reserve (url) {\n      this.loading = true\n      setTimeout(() => {\n            this.loading = false\n            window.open(url)\n          }\n      , 2000)\n    },\n  },\n};\n</script>\n\n<style scoped>\n</style>\n"]}]}
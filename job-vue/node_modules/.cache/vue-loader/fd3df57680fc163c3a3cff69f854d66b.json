AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DotChart.vue","sourceRoot":"src/views/Charts/Dash4","sourcesContent":["<template>\n  <v-chart\n    style=\"width:100%; min-height:650px;\"\n    :options=\"chartOption\"\n    autoresize\n  />\n</template>\n\n<script>\nimport {getTypeRate} from \"@/api/job\";\n\nexport default {\n  name: 'BaseBarChart',\n  data() {\n    return {\n      chartOption: {},\n      d1 :[],\n      d2 :[],\n      d3 :[],\n      d4 :[],\n      d5 :[],\n      l1 :[]\n    };\n  },\n  async mounted() {\n    await getTypeRate().then(res => {\n      // console.log(res.data.data)\n      this.d1 = res.data.data.datas[0]\n      this.d2 = res.data.data.datas[1]\n      this.d3 = res.data.data.datas[2]\n      this.d4 = res.data.data.datas[3]\n      this.d5 = res.data.data.datas[4]\n      this.l1 = res.data.data.labels\n      console.log(this.l1)\n    })\n    this.chartOption = this.buildChartOption();\n  },\n  methods: {\n    buildChartOption() {\n      const option = {\n        color: ['#fec42c', '#80F1BE', '#64B5F6','#455A64','#DD2C00'],\n        legend: {\n          data: this.l1,\n          left: 'center',\n          bottom: 10,\n          textStyle:{\n            fontSize: 20,\n            color: '#F1F1F3'\n          }\n        },\n        tooltip: {\n          backgroundColor: 'rgba(255,255,255,0.7)',\n          formatter: function (param) {\n            var value = param.value;\n            // prettier-ignore\n            return '<div style=\"border-bottom: 1px solid rgba(255,255,255,.3); font-size: 18px;padding-bottom: 7px;margin-bottom: 7px\">'\n                + param.seriesName + ' '  + '学历：'\n                + value[2]\n                + '</div>'\n                + '职位：' + param.value[4] + '<br>'\n                + '公司：' + param.value[5] + '<br>'\n                + '最高月薪：' + param.value[1] + '<br>'\n                + '最低月薪：' + param.value[3] + '<br>'\n          },\n            axisPointer: {\n              show: true,\n              type: 'cross',\n              lineStyle: {\n                type: 'dashed',\n                width: 1\n              }\n            }\n        },\n        // tooltip: {\n        //   // trigger: 'axis',\n        //   showDelay: 0,\n        //   formatter: function (params) {\n        //     if (params.value.length > 1) {\n        //       return (\n        //           params.seriesName +\n        //           ' :<br/>' +\n        //           params.value[2] + ',最高月薪' +\n        //           params.value[1] +\n        //           '元'\n        //       );\n        //     }\n        //   },\n        //   axisPointer: {\n        //     show: true,\n        //     type: 'cross',\n        //     lineStyle: {\n        //       type: 'dashed',\n        //       width: 1\n        //     }\n        //   }\n        // },\n        xAxis: {\n          type: 'category',   //这个分为category类目,value数值,time,时间和log对数,选择的不对可能出现无法对应的x轴问题\n          scale: true,\n          data: [\"\",\"博士\",\"\",\"硕士\",\"本科\",\"大专\",\"\",\"高中\",\"\",\"初中及以下\"],\n          axisLabel: {\n            show: true,\n            fontSize: 14,\n            color: '#F1F1F3',\n            // formatter: function (number){\n            //   return number + '学历'\n            // }\n          }\n        },\n        yAxis: {\n          scale: true,\n          axisLine: {\n            lineStyle: {\n              color: '#78909C',\n            },\n          },\n          axisLabel: {\n            show: true,\n            fontSize: 14,\n            color: '#F1F1F3'\n          },\n        },\n        series: [\n          {\n            name : this.l1[0],\n            symbolSize: function (dataItem) {\n              // console.log(dataItem)\n              return dataItem[1] / 6000;\n            },\n            emphasis: {\n              focus: 'series'\n            },\n            markArea: {\n              silent: true,\n              itemStyle: {\n                color: 'transparent',\n                borderWidth: 1,\n                borderType: 'dashed'\n              },\n              data: [\n                [\n                  {\n                    name: '薪酬区间',\n                    xAxis: 'min',\n                    yAxis: 'min'\n                  },\n                  {\n                    xAxis: 'max',\n                    yAxis: 'max'\n                  }\n                ]\n              ]\n            },\n            data: this.d1,\n\n                // [\n              // [2001, 8.04],\n              // [2002, 6.95],\n              // [2003, 7.58],\n              // [2004, 8.81],\n              // [1998, 8.33],\n              // [1997, 7.66],\n              // [1997, 6.81],\n              // [1995, 6.33],\n              // [1993, 8.96],\n              // [1992, 6.82],\n              // [1991, 7.2],\n              // [1994, 7.2],\n              // [2021, 4.23],\n              // [1950, 7.83],\n              // [1948, 4.47],\n              // [2010, 3.33],\n              // [1999, 4.96],\n              // [2011, 7.24],\n              // [2019, 6.26],\n              // [2013, 8.84],\n              // [1987, 5.82],\n              // [1983, 5.68]\n            // ],\n            type: 'scatter'\n          },\n          {\n            name : this.l1[1],\n            symbolSize: function (dataItem) {\n              // console.log(dataItem)\n              return dataItem[1] / 6000;\n            },\n            data: this.d2,\n            type: 'scatter',\n            emphasis: {\n              focus: 'series'\n            },\n          },\n          {\n            name : this.l1[2],\n            symbolSize: function (dataItem) {\n              // console.log(dataItem)\n              return dataItem[1] / 6000;\n            },\n            data: this.d3,\n            type: 'scatter',\n            emphasis: {\n              focus: 'series'\n            },\n          },\n          {\n            name : this.l1[3],\n            symbolSize: function (dataItem) {\n              // console.log(dataItem)\n              return dataItem[1] / 6000;\n            },\n            data: this.d4,\n            type: 'scatter',\n            emphasis: {\n              focus: 'series'\n            },\n          },\n          {\n            name : this.l1[4],\n            symbolSize: function (dataItem) {\n              // console.log(dataItem)\n              return dataItem[1] / 6000;\n            },\n            data: this.d5,\n            type: 'scatter',\n            emphasis: {\n              focus: 'series'\n            },\n          }\n        ],\n      };\n      return option;\n    },\n  },\n};\n</script>\n"]}]}